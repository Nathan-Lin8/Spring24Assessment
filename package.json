{
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "concurrently": "^8.2.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.2"
  },
  "scripts": {
    "postinstall": "concurrently yarn:postinstall:*",
